{
  "name": "@omnidev/sigil",
  "description": "Omni Sigil design system and library",
  "author": "Brian Cooper <brian@omni.dev>",
  "version": "0.0.0",
  "license": "MIT",
  "packageManager": "yarn@3.6.2",
  "main": "build/sigil.js",
  "module": "build/sigil.mjs",
  "types": "build/sigil.d.ts",
  "files": [
    "build"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/omnidotdev/sigil"
  },
  "exports": {
    ".": {
      "require": "./build/sigil.js",
      "import": "./build/sigil.mjs",
      "types": "./build/sigil.d.ts"
    },
    "./index.css": "./build/sigil.css"
  },
  "scripts": {
    "dev": "dotenv -e .env -c -- panda codegen && concurrently --kill-others -n \"PANDA,STORYBOOK\" -c \"#ffc83d,#ff4785\" \"yarn dev:css\" \"yarn storybook\"",
    "dev:css": "panda --watch",
    "build": "panda codegen && tsup",
    "format": "prettier . --write",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src",
    "test": "panda codegen && jest",
    "test:coverage": "yarn test --coverage",
    "test:ci": "yarn test --ci",
    "test:watch": "yarn test --watch",
    "storybook": "storybook dev --quiet --no-open -p 6006",
    "storybook:build": "panda codegen && storybook build",
    "studio": "panda studio",
    "studio:build": "panda codegen && panda studio --build",
    "changeset": "changeset",
    "release": "yarn build && changeset publish",
    "postinstall": "husky install",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@ark-ui/anatomy": "^0.1.0",
    "@ark-ui/react": "^0.15.0",
    "@changesets/changelog-github": "^0.4.8",
    "@changesets/cli": "^2.26.2",
    "@commitlint/cli": "^17.7.2",
    "@commitlint/config-conventional": "^17.7.0",
    "@commitlint/types": "^17.4.4",
    "@jest/globals": "^29.7.0",
    "@omnidev/knit": "^0.1.2",
    "@pandacss/dev": "^0.15.5",
    "@storybook/addon-a11y": "^7.4.6",
    "@storybook/addon-docs": "^7.4.6",
    "@storybook/addon-essentials": "^7.4.6",
    "@storybook/addon-interactions": "^7.4.6",
    "@storybook/addon-links": "^7.4.6",
    "@storybook/addon-storysource": "^7.4.6",
    "@storybook/blocks": "^7.4.6",
    "@storybook/manager-api": "^7.4.6",
    "@storybook/react": "^7.4.6",
    "@storybook/react-vite": "^7.4.6",
    "@storybook/testing-library": "^0.2.2",
    "@storybook/theming": "^7.4.6",
    "@storybook/types": "^7.4.6",
    "@swc/core": "^1.3.92",
    "@swc/jest": "^0.2.29",
    "@testing-library/dom": "^9.3.3",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/node": "^20.8.4",
    "@types/react": "^18.2.28",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "@zag-js/accordion": "^0.19.1",
    "@zag-js/carousel": "^0.19.1",
    "@zag-js/color-picker": "^0.19.1",
    "@zag-js/color-utils": "^0.19.1",
    "@zag-js/combobox": "^0.19.1",
    "@zag-js/date-picker": "^0.19.1",
    "@zag-js/menu": "^0.19.1",
    "@zag-js/pagination": "^0.19.1",
    "@zag-js/radio-group": "^0.19.1",
    "@zag-js/rating-group": "^0.19.1",
    "@zag-js/rect-utils": "0.19.1",
    "@zag-js/select": "^0.19.1",
    "@zag-js/splitter": "^0.19.1",
    "@zag-js/tabs": "0.19.1",
    "@zag-js/toast": "^0.19.1",
    "concurrently": "^8.2.1",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jest": "^27.4.2",
    "eslint-plugin-jest-dom": "^5.1.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.15",
    "eslint-plugin-testing-library": "^6.0.2",
    "eslint-plugin-typescript-sort-keys": "^3.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^14.0.1",
    "pinst": "^3.0.0",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.11.0",
    "storybook": "^7.4.6",
    "storybook-dark-mode": "^3.0.1",
    "ts-node": "^10.9.1",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "usehooks-ts": "^2.9.1",
    "vite": "^4.4.11",
    "vite-tsconfig-paths": "^4.2.1"
  }
}
